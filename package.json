{
  "name": "build-your-own",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx, js, jsx, md, mdx, css}\"",
    "prepare": "husky install",
    "dev:docker": "npm run prisma:docker && npm run dev",
    "prisma:docker": "cd packages/db && npx prisma migrate dev --skip-seed"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,md,mdx,css}": [
      "npm run format",
      "npm run lint"
    ]
  },
  "devDependencies": {
    "husky": "^8.0.0",
    "prettier": "^3.2.5",
    "turbo": "^2.1.1",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.5.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "passport-google-oauth2": "^0.2.0",
    "ts-node": "^10.9.2"
  }
}
